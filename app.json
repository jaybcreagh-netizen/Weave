{
  "expo": {
    "name": "Weave",
    "slug": "weave",
    "version": "1.0.1",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#6366F1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaycreagh.WeaveNative",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSContactsUsageDescription": "Weave uses your contacts to help you select friends to add to your circles.",
        "NSCalendarsUsageDescription": "Weave adds your planned interactions to your calendar so you never miss a connection.",
        "NSRemindersUsageDescription": "Weave can set reminders for your planned interactions.",
        "UIBackgroundModes": [
          "fetch"
        ]
      },
      "entitlements": {
        "com.apple.developer.icloud-container-identifiers": [
          "iCloud.com.jaycreagh.WeaveNative"
        ],
        "com.apple.developer.icloud-services": [
          "CloudDocuments"
        ],
        "com.apple.developer.ubiquity-container-identifiers": [
          "iCloud.com.jaycreagh.WeaveNative"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6366F1"
      },
      "package": "com.jaycreagh.WeaveNative",
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "READ_CONTACTS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "weave"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "weave",
    "newArchEnabled": false,
    "extra": {},
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "weave-native",
          "organization": "jay-creagh"
        }
      ]
    ]
  }
}