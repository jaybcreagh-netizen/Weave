{
  "expo": {
    "name": "WeaveNative",
    "slug": "WeaveNative",
    "version": "1.0.0", 
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaycreagh.WeaveNative",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
