{
  "expo": {
    "name": "WeaveNative",
    "slug": "WeaveNative",
    "version": "1.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaycreagh.WeaveNative",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSContactsUsageDescription": "Weave uses your contacts to help you select friends to add to your circles."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jaycreagh.WeaveNative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "weavenative",
    "newArchEnabled": true,
    "extra": {},
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
