{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {
      "@/modules/*": [
        "src/modules/*"
      ],
      "@/shared/*": [
        "src/shared/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "@/db": [
        "src/db/index"
      ],
      "@/db/*": [
        "src/db/*"
      ],
      "@/app/*": [
        "app/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/stores/*": [
        "src/stores/*"
      ],
      "@/context/*": [
        "src/context/*"
      ],
      "@/guidelines/*": [
        "src/guidelines/*"
      ],
      "@/repositories/*": [
        "src/repositories/*"
      ],
      "@/assets/*": [
        "assets/*"
      ]
    }
  },
  "exclude": [
    "supabase/functions"
  ]
}